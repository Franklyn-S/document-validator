'form-control'\n              onChange={e => setFileId(e.target.value)}\n            />\n          </div>\n          <div className='form-group'>\n            <label htmlFor='fileId' className='control-label'>\n              Motivação da validação:\n            </label>\n            <input\n              type='text'\n              id='fileId'\n              name='fileId'\n              value={motivation}\n              className='form-control'\n              onChange={e => setMotivation(e.target.value)}\n            />\n          </div>\n          <div className='form-group'>\n            <label htmlFor='file' className='control-label'>\n              Arquivo:\n            </label>\n            <input\n              type='file'\n              id='file'\n              name='file'\n              onChange={e => {\n                const file = e.target.files[0];\n                setFile(file);\n                generateBase64(file);\n              }}\n            />\n          </div>\n          <button className='btn btn-primary'>Validar</button>\n        </ValidationFormStyle>\n      </div>\n    </div>\n  );\n};\n\nconst ValidationFormStyle = styled.form`\n  display: block;\n  margin: auto;\n`;\n\nexport default ValidateDocument;\n","D:\\College\\8 semestre\\Cloud Development\\document-validat\\document-validator\\src\\screens\\Documents.js",[],"D:\\College\\8 semestre\\Cloud Development\\document-validat\\document-validator\\src\\components\\Card\\index.js",[],"D:\\College\\8 semestre\\Cloud Development\\document-validat\\document-validator\\src\\components\\Alert\\index.js",[],"D:\\College\\8 semestre\\Cloud Development\\document-validat\\document-validator\\src\\services.js",[],"D:\\College\\8 semestre\\Cloud Development\\document-validat\\document-validator\\src\\components\\RegisterForm\\styled.js",[],"D:\\College\\8 semestre\\Cloud Development\\document-validat\\document-validator\\src\\components\\Modals\\AddFileModal.js",[],"D:\\College\\8 semestre\\Cloud Develo